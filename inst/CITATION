citHeader("To cite iterLap in publications use:")

year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "Manual",
         title = "iterLap: Iterated Laplace Approximations",
         author = person("Bjoern", "Bornkamp"),
         year = year,
         note = note,
         url = "https://CRAN.R-project.org/package=iterLap")

bibentry(
  bibtype="Article",
  title="Approximating Probability Densities by Iterated
  Laplace Approximations",
  journal="Journal of Computational and Graphical Statistics",
  volume= "20",
  pages="656-669",
  year="2011",
  author=person("Bjoern", "Bornkamp"))

